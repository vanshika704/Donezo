{
    "version": 2,
    "routes": [
      {
        "src": "/sw.js",
        "dest": "/sw.js",
        "headers": {
          "Service-Worker-Allowed": "/",
          "Cache-Control": "no-cache"
        }
      },
      {
        "src": "/assets/(.*)",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "headers": {
          "Cache-Control": "no-cache"
        }
      }
    ]
  }